{
	"parser": "babel-eslint",
	"plugins": ["react", "react-native"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true,
			"modules": true
		}
	},
	"env": {
		"es6": true,
		"node": true,
		"browser": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"rules": {
		"arrow-body-style": "warn",
		"no-console": "off",
		"react/prop-types": "off"
	}
}
